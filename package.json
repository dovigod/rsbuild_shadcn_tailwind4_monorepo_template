{
  "name": "react19-tw4-shadcn-rsbuild-template",
  "version": "0.0.0",
  "description": "",
  "author": "dovigod",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {},
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "server",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.4.0",
  "engines": {
    "node": ">=22"
  }
}
